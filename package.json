    "autoprefixer": "^10.4.20",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.38.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "5.6.3",
    "vite": "^5.4.20",
    "vitest": "^4.0.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --config config/eslint.config.js --fix --max-warnings 500"
