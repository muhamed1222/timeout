{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "scripts": {
    "docs:tree": "node scripts/generate-repo-tree.mjs",
    "dev": "node concurrent-dev.cjs",
    "build": "vite build",
    "build:vercel": "vite build && (tsc --project tsconfig.server.json --skipLibCheck || true) && cp -r shared dist/",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "check-deploy": "node scripts/check-deploy.cjs",
    "db:push": "drizzle-kit push",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:integration:watch": "vitest tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "optimize:images": "tsx scripts/maintenance/optimize-images.ts",
    "db:backup": "./scripts/db/backup-database.sh production",
    "db:restore": "./scripts/db/restore-database.sh",
    "db:verify": "./scripts/db/verify-backup.sh",
    "db:rollback": "./migrations/rollback.sh",
    "db:health": "tsx scripts/monitor/check-database-health.ts",
    "db:analyze-indexes": "tsx scripts/db/analyze-indexes.ts",
    "db:analyze-queries": "tsx scripts/analyze-slow-queries.ts",
    "prepare": "husky",
    "lint": "eslint . --config config/eslint.config.js --max-warnings 500",
    "lint:fix": "eslint . --config config/eslint.config.js --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md,yml,yaml}\"",
    "type-check": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run type-check"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.922.0",
